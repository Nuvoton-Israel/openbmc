			&gpio67i_pins
			&gpio68i_pins
			&gpio69i_pins
			&gpio70i_pins
			&gpio71oh_pins
			&gpio72i_pins
			&gpio73oh_pins
			&gpio74i_pins
			&gpio75i_pins
			&gpio76ol_pins
			&gpio77i_pins
			&gpio78ol_pins
			&gpio79i_pins
			&gpio81oh_pins
			&gpio82oh_pins
			&gpio83oh_pins
			&gpio84i_pins
			&gpio89i_pins
			&gpio91i_pins
			&gpio92i_pins
			&gpio93oh_pins
			&gpio94oh_pins
			&gpio114i_pins
			&gpio115i_pins
			&gpio116ol_pins
			&gpio117oh_pins
			&gpio120i_pins
			&gpio121ol_pins
			&gpio122oh_pins
			&gpio123i_pins
			&gpio144ol_pins
			&gpio145i_pins
			&gpio146i_pins
			&gpio147i_pins
			&gpio160oh_pins
			&gpio162i_pins
			&gpio190oh_pins
			&gpio195ol_pins
			&gpio196ol_pins
			&gpio197ol_pins
			&gpio198oh_pins
			&gpio199oh_pins
			&gpio200i_pins
